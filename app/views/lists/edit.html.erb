<div class="row">
  <div class="col-md-8 col-md-offset-2">
  <h1 lass="page-header">Edit List</h1>
    <%= form_for @list do |f| %>
    <div class="form-group">
      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control', placeholder: 'Name your list' %>
    </div>
    <div class="form-group">
      <%= f.submit 'Save', class: 'btn btn-success' %>
    </div>
    <% end %>
  </div>
</div>